@model WebApplication.Models.organizations

@{
    ViewBag.Title = "Подробно";
}

<h2>Подробно</h2>

<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>Название</dt>
        <dd>
            @Html.DisplayFor(model => model.name)
        </dd>

        <dt>Телефон</dt>
        <dd>
            @Html.DisplayFor(model => model.phone)
        </dd>

        <dt>E-mail</dt>
        <dd>
            @Html.DisplayFor(model => model.email)
        </dd>

        <dt>Заметки</dt>
        <dd>
            @Html.DisplayFor(model => model.note)
        </dd>

    </dl>
</div>
<p>
    @Html.ActionLink("Изменить", "Edit", new { id = Model.organizationID }) |
    @Html.ActionLink("Удалить", "Delete", new { id = Model.organizationID }) |
    @Html.ActionLink("Назад к списку организаций", "Index")
</p>
